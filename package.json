{
  "name": "jjwm-portfolio-client",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "build": "babel-node tools/build.js && npm run open:dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "copy-worker": "cp node_modules/pdfjs-dist/build/pdf.worker.js dist/pdf.worker.js",
    "heroku-postbuild": "babel-node tools/build.js",
    "heroku-prebuild": "",
    "lint": "esw webpack.config.* src tools --color",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "open:dist": "babel-node tools/distServer.js",
    "open:src": "babel-node tools/srcServer.js",
    "prebuild": "npm run lint && npm run test && npm run clean-dist && npm run copy-worker",
    "preinstall": "node tools/nodeVersionCheck.js",
    "prestart": "npm run start-message",
    "remove-dist": "rimraf ./dist",
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\" \"npm run lint:watch\"",
    "start-message": "babel-node tools/startMessage.js",
    "start:dev": "concurrently -k -r -s first \"npm run open:src\"",
    "test": "jest",
    "test:CI": "jest",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watchAll"
  },
  "author": "Jack McGregor",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.7.4",
    "@hot-loader/react-dom": "16.8.6",
    "connected-react-router": "6.4.0",
    "dotenv": "8.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "express": "4.17.1",
    "jwt-decode": "2.2.0",
    "object-assign": "4.1.1",
    "prettier": "1.19.1",
    "react": "16.8.6",
    "react-addons-css-transition-group": "15.6.2",
    "react-animations": "1.0.0",
    "react-dom": "16.8.6",
    "react-modal": "3.11.1",
    "react-pdf": "4.1.0",
    "react-redux": "7.0.3",
    "react-router-dom": "^5.1.2",
    "react-virtualized": "9.21.1",
    "redux": "4.0.1",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "redux-thunk": "2.3.0",
    "styled-components": "4.2.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-runtime": "7.7.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/types": "7.4.4",
    "acorn": "6.1.1",
    "ajv": "6.10.0",
    "autoprefixer": "9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "browser-sync": "2.26.7",
    "chalk": "2.4.2",
    "concurrently": "4.1.0",
    "connect-history-api-fallback": "1.6.0",
    "copy-webpack-plugin": "5.0.5",
    "coveralls": "3.0.3",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.13.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-watch": "5.1.2",
    "file-loader": "3.0.1",
    "hard-source-webpack-plugin": "0.13.1",
    "history": "4.9.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.6.0",
    "mockdate": "2.0.2",
    "node-sass": "4.13.1",
    "opn-cli": "4.1.0",
    "postcss-loader": "3.0.0",
    "prompt": "1.0.0",
    "prop-types": "15.7.2",
    "raf": "3.4.1",
    "react-hot-loader": "4.8.4",
    "react-test-renderer": "16.8.6",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "replace": "1.1.0",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.31.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-dev-middleware": "3.6.2",
    "webpack-hot-middleware": "2.24.4"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(scss|css)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "react-hot-loader/babel",
          [
            "@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "ie": 9
              },
              "forceAllTransforms": true
            }
          ],
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-react-constant-elements",
          "transform-react-remove-prop-types",
          [
            "@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          [
            "@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "globals": {}
  }
}
